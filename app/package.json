{
  "name": "node-fluent-api",
  "version": "1.0.0",
  "description": "Extração de texto de PDF com regex usando o padrão Fluent API",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "npx nodemon --ignore database.json --exec node --experimental-json-modules --experimental-top-level-await src/index.js",
    "test": "npx mocha -w --parallel test/*.test.js",
    "test:cov": "npx nyc npx mocha -r reify --parallel test/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaximilianoOliveiraFurtado/node-fluent-api.git"
  },
  "author": "Maximiliano de Oliveira Furtado",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MaximilianoOliveiraFurtado/node-fluent-api/issues"
  },
  "homepage": "https://github.com/MaximilianoOliveiraFurtado/node-fluent-api#readme",
  "devDependencies": {
    "chai": "^4.3.0",
    "mocha": "^8.3.0",
    "nyc": "^15.1.0"
  },
  "dependencies": {
    "pdf-parse": "^1.1.1"
  }
}
